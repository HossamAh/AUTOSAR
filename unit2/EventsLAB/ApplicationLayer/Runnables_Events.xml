<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://autosar.org/schema/r4.0 file:///I:/AUTOSAR_learn-in-depth/unit1/understanding_ARXML/AUTOSARSchema/AUTOSAR_00046.xsd">

	<AR-PACKAGES>
		<AR-PACKAGE>
			<SHORT-NAME>SWCs</SHORT-NAME>
			<ELEMENTS>
				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>SWC1</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>Output_PPort</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Interfaces/SRIF_Output</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>Inputs_RPort</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Interfaces/SRIF_Inputs</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
					</PORTS>
					<INTERNAL-BEHAVIORS>
						<SWC-INTERNAL-BEHAVIOR>
							<SHORT-NAME>SWC1_internal_behaviour</SHORT-NAME>
							<EVENTS>
								<TIMING-EVENT>
									<SHORT-NAME>Time10ms</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/SWCs/SWC1/SUM_runnable</START-ON-EVENT-REF>
									<PERIOD>0.01</PERIOD>
								</TIMING-EVENT>
								<INIT-EVENT>
									<SHORT-NAME>SWC1_initEvent</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/SWCs/SWC1/SUM_runnable</START-ON-EVENT-REF>
								</INIT-EVENT>
								<DATA-RECEIVED-EVENT>
									<SHORT-NAME>SWC1_DRC_Val1</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/SWCs/SWC1/SUM_runnable</START-ON-EVENT-REF>
									<DATA-IREF>
										<CONTEXT-R-PORT-REF DEST="R-PORT-PROTOTYPE">/SWCs/SWC1/Inputs_RPort</CONTEXT-R-PORT-REF>
										<TARGET-DATA-ELEMENT-REF
											DEST="VARIABLE-DATA-PROTOTYPE">/Interfaces/SRIF_Inputs/val1</TARGET-DATA-ELEMENT-REF>
									</DATA-IREF>
								</DATA-RECEIVED-EVENT>
								<DATA-SEND-COMPLETED-EVENT>
									<SHORT-NAME>SumResult_WriteCompleteEvent</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/SWCs/SWC1/SUM_runnable</START-ON-EVENT-REF>
									<EVENT-SOURCE-REF DEST="VARIABLE-ACCESS">/SWCs/SWC1/SUM_runnable/SumResult</EVENT-SOURCE-REF>
								</DATA-SEND-COMPLETED-EVENT>
							</EVENTS>
							
							<RUNNABLES>
								<RUNNABLE-ENTITY>
									<SHORT-NAME>SUM_runnable</SHORT-NAME>
									<MINIMUM-START-INTERVAL>0.0</MINIMUM-START-INTERVAL>
									<CAN-BE-INVOKED-CONCURRENTLY>false</CAN-BE-INVOKED-CONCURRENTLY>
									<DATA-READ-ACCESSS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>val1</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF
														DEST="R-PORT-PROTOTYPE">/SWCs/SWC1/Inputs_RPort</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF
														DEST="VARIABLE-DATA-PROTOTYPE">/Interfaces/SRIF_Inputs/var1</TARGET-DATA-PROTOTYPE-REF>
												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>val2</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF
														DEST="R-PORT-PROTOTYPE">/SWCs/SWC1/Inputs_RPort</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF
														DEST="VARIABLE-DATA-PROTOTYPE">/Interfaces/SRIF_Inputs/var2</TARGET-DATA-PROTOTYPE-REF>
												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
									</DATA-READ-ACCESSS>
									<DATA-WRITE-ACCESSS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>SumResult</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF
														DEST="P-PORT-PROTOTYPE">/SWCS/SWC1/Output_PPort</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF
														DEST="VARIABLE-DATA-PROTOTYPE">/Interfaces/SRIF_Output/SumResult</TARGET-DATA-PROTOTYPE-REF>
												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
									</DATA-WRITE-ACCESSS>

									<SYMBOL>SUM</SYMBOL>

								</RUNNABLE-ENTITY>
							</RUNNABLES>

						</SWC-INTERNAL-BEHAVIOR>
					</INTERNAL-BEHAVIORS>
				</APPLICATION-SW-COMPONENT-TYPE>
			</ELEMENTS>
		</AR-PACKAGE>
		<AR-PACKAGE>
			<SHORT-NAME>Interfaces</SHORT-NAME>
			<ELEMENTS>
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRIF_Inputs</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>val1</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>val2</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRIF_Output</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>SumResult</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>

			</ELEMENTS>

		</AR-PACKAGE>
	</AR-PACKAGES>
</AUTOSAR>