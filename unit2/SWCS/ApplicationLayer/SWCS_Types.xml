<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://autosar.org/schema/r4.0 file:///I:/AUTOSAR_learn-in-depth/unit1/understanding_ARXML/AUTOSARSchema/AUTOSAR_00046.xsd">

	<AR-PACKAGES>
		<AR-PACKAGE>
			<SHORT-NAME>SWCs</SHORT-NAME>
			<ELEMENTS>
				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>EABS_SWC</SHORT-NAME>
					<PORTS>
						<!-- Read the lidar data to Application SWC -->
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>LIDAR_DATA_R_Port</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Interfaces/LIDAR_SA_SRIF</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
						<!-- Read the Speed data to Application SWC -->
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>SPEED_DATA_R_Port</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Interfaces/SPEED_SA_SRIF</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
						<!-- receive the clibration parameters -->
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>Parameters_R_Port</SHORT-NAME>
							<PARAMETER-PORT-ANNOTATIONS>
								<PARAMETER-PORT-ANNOTATION>
									<PARAMETER-REF DEST="PARAMETER-DATA-PROTOTYPE">/Interfaces/CLIBRATION_DISTANCE_PARAMETERIF/D1</PARAMETER-REF>
								</PARAMETER-PORT-ANNOTATION>
								<PARAMETER-PORT-ANNOTATION>
									<PARAMETER-REF DEST="PARAMETER-DATA-PROTOTYPE">/Interfaces/CLIBRATION_DISTANCE_PARAMETERIF/D2</PARAMETER-REF>
								</PARAMETER-PORT-ANNOTATION>
								<PARAMETER-PORT-ANNOTATION>
									<PARAMETER-REF DEST="PARAMETER-DATA-PROTOTYPE">/Interfaces/CLIBRATION_DISTANCE_PARAMETERIF/D3</PARAMETER-REF>
								</PARAMETER-PORT-ANNOTATION>
							</PARAMETER-PORT-ANNOTATIONS>
						</R-PORT-PROTOTYPE>
						<!-- send the Action and sensor data to NV block SWC to save in flash -->
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>NV_DATA_P_PORT</SHORT-NAME>
							<NV-DATA-PORT-ANNOTATIONS>
								<NV-DATA-PORT-ANNOTATION>
									<VARIABLE-REF DEST="VARIABLE-DATA-PROTOTYPE">/Interfaces/NV_DATA_IF/Action</VARIABLE-REF>
								</NV-DATA-PORT-ANNOTATION>
								<NV-DATA-PORT-ANNOTATION>
									<VARIABLE-REF DEST="VARIABLE-DATA-PROTOTYPE">/Interfaces/NV_DATA_IF/LIDAR_Data</VARIABLE-REF>
								</NV-DATA-PORT-ANNOTATION>
								<NV-DATA-PORT-ANNOTATION>
									<VARIABLE-REF DEST="VARIABLE-DATA-PROTOTYPE">/Interfaces/NV_DATA_IF/Speed_Data</VARIABLE-REF>
								</NV-DATA-PORT-ANNOTATION>
							</NV-DATA-PORT-ANNOTATIONS>
						</P-PORT-PROTOTYPE>
						<!-- log the action to DCM through client server interface -->
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>APP_DCM_CLIENT_PORT</SHORT-NAME>
							<CLIENT-SERVER-ANNOTATIONS>
								<CLIENT-SERVER-ANNOTATION>
									<OPERATION-REF DEST="CLIENT-SERVER-OPERATION">/Interfaces/APP_DCM_LOG_CSIF/LOG_Action</OPERATION-REF>
								</CLIENT-SERVER-ANNOTATION>
							</CLIENT-SERVER-ANNOTATIONS>
						</R-PORT-PROTOTYPE>

						<!-- client port from app to CDD to perform actions -->
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>APP_CDD_ACTIONS_CLIENT_PORT</SHORT-NAME>
							<CLIENT-SERVER-ANNOTATIONS>
								<CLIENT-SERVER-ANNOTATION>
									<OPERATION-REF DEST="CLIENT-SERVER-OPERATION">/Interfaces/APP_CDD_ACTIONS_CSIF/SLOWDOWN_Action</OPERATION-REF>
								</CLIENT-SERVER-ANNOTATION>

								<CLIENT-SERVER-ANNOTATION>
									<OPERATION-REF DEST="CLIENT-SERVER-OPERATION">/Interfaces/APP_CDD_ACTIONS_CSIF/BARKING_Action</OPERATION-REF>
								</CLIENT-SERVER-ANNOTATION>
							</CLIENT-SERVER-ANNOTATIONS>
						</R-PORT-PROTOTYPE>

						<!-- client port from app to SA to perform warning on Led -->
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>APP_SA_WARNING_CLIENT_PORT</SHORT-NAME>
							<CLIENT-SERVER-ANNOTATIONS>
								<CLIENT-SERVER-ANNOTATION>
									<OPERATION-REF DEST="CLIENT-SERVER-OPERATION">/Interfaces/APP_SA_WARNING_CSIF/WARNING_ON</OPERATION-REF>
								</CLIENT-SERVER-ANNOTATION>

								<CLIENT-SERVER-ANNOTATION>
									<OPERATION-REF DEST="CLIENT-SERVER-OPERATION">/Interfaces/APP_SA_WARNING_CSIF/WARNING_OFF</OPERATION-REF>
								</CLIENT-SERVER-ANNOTATION>
							</CLIENT-SERVER-ANNOTATIONS>
						</R-PORT-PROTOTYPE>


					</PORTS>
				</APPLICATION-SW-COMPONENT-TYPE>


				<PARAMETER-SW-COMPONENT-TYPE>
					<SHORT-NAME>DISTANCE_PARAMTERS_SWC</SHORT-NAME>
					<PORTS>
						<!-- send the clibration parameters -->
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>Parameters_R_Port</SHORT-NAME>
							<PARAMETER-PORT-ANNOTATIONS>
								<PARAMETER-PORT-ANNOTATION>
									<PARAMETER-REF DEST="PARAMETER-DATA-PROTOTYPE">/CLIBRATION_DISTANCE_PARAMETERIF/D1</PARAMETER-REF>
								</PARAMETER-PORT-ANNOTATION>
								<PARAMETER-PORT-ANNOTATION>
									<PARAMETER-REF DEST="PARAMETER-DATA-PROTOTYPE">/CLIBRATION_DISTANCE_PARAMETERIF/D2</PARAMETER-REF>
								</PARAMETER-PORT-ANNOTATION>
								<PARAMETER-PORT-ANNOTATION>
									<PARAMETER-REF DEST="PARAMETER-DATA-PROTOTYPE">/CLIBRATION_DISTANCE_PARAMETERIF/D3</PARAMETER-REF>
								</PARAMETER-PORT-ANNOTATION>
							</PARAMETER-PORT-ANNOTATIONS>
						</P-PORT-PROTOTYPE>
					</PORTS>
				</PARAMETER-SW-COMPONENT-TYPE>

				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>LIDAR_SENSOR_SWC</SHORT-NAME>
					<PORTS>
						<!-- Receive the lidar data from sensor via ECU abstraction -->
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>LIDAR_DATA_R_Port</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Interfaces/LIDAR_SA_SRIF</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
						<!-- send the received lidar data to Application SWC -->
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>LIDAR_DATA_P_Port</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Interfaces/LIDAR_SA_SRIF</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>

					</PORTS>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>


				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>SPEED_SENSOR_SWC</SHORT-NAME>
					<PORTS>
						<!-- Receive the Speed data from sensor via ECU abstraction -->
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>SPEED_DATA_R_Port</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Interfaces/SPEED_SA_SRIF</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
						<!-- send the received Speed data to Application SWC -->
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>SPEED_DATA_P_Port</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Interfaces/SPEED_SA_SRIF</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>

					</PORTS>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>


				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>WarningLED_ACTUATOR_SWC</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>Warning_state_P_PORT</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Interfaces/Warning_LED_State_SA_ECU_ABS_SRIF</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
					</PORTS>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>

				<ECU-ABSTRACTION-SW-COMPONENT-TYPE>
					<SHORT-NAME>SASWC_ECU_ABstraction_SWC</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>LIDAR_DATA_P_Port</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Interfaces/LIDAR_SA_SRIF</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>SPEED_DATA_P_Port</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Interfaces/LIDAR_SA_SRIF</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>Warning_state_P_PORT</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/Interfaces/Warning_LED_State_SA_ECU_ABS_SRIF</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>

					</PORTS>
				</ECU-ABSTRACTION-SW-COMPONENT-TYPE>

				<COMPLEX-DEVICE-DRIVER-SW-COMPONENT-TYPE>
					<SHORT-NAME>SLOWDOWN_BARKING_CDD_SWC</SHORT-NAME>
				</COMPLEX-DEVICE-DRIVER-SW-COMPONENT-TYPE>

				<NV-BLOCK-SW-COMPONENT-TYPE>
					<SHORT-NAME>SAVE_ACTION_SENSOR_VALUE_NVBlock_SWC</SHORT-NAME>
					<PORTS>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>NV_DATA_R_PORT</SHORT-NAME>
							<NV-DATA-PORT-ANNOTATIONS>
								<NV-DATA-PORT-ANNOTATION>
									<VARIABLE-REF DEST="VARIABLE-DATA-PROTOTYPE">/Interfaces/NV_DATA_IF/Action</VARIABLE-REF>
								</NV-DATA-PORT-ANNOTATION>
								<NV-DATA-PORT-ANNOTATION>
									<VARIABLE-REF DEST="VARIABLE-DATA-PROTOTYPE">/Interfaces/NV_DATA_IF/LIDAR_Data</VARIABLE-REF>
								</NV-DATA-PORT-ANNOTATION>
								<NV-DATA-PORT-ANNOTATION>
									<VARIABLE-REF DEST="VARIABLE-DATA-PROTOTYPE">/Interfaces/NV_DATA_IF/Speed_Data</VARIABLE-REF>
								</NV-DATA-PORT-ANNOTATION>
							</NV-DATA-PORT-ANNOTATIONS>
						</R-PORT-PROTOTYPE>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>NVBlock_NVM_Client_PORT</SHORT-NAME>
							<CLIENT-SERVER-ANNOTATIONS>
								<CLIENT-SERVER-ANNOTATION>
									<OPERATION-REF DEST="CLIENT-SERVER-OPERATION">/Interfaces/NVBLOCK_SWC_NVMM_CSIF/SAVE_Action</OPERATION-REF>
								</CLIENT-SERVER-ANNOTATION>
								<CLIENT-SERVER-ANNOTATION>
									<OPERATION-REF DEST="CLIENT-SERVER-OPERATION">/Interfaces/NVBLOCK_SWC_NVMM_CSIF/SAVE_LIDAR_Data</OPERATION-REF>
								</CLIENT-SERVER-ANNOTATION>
								<CLIENT-SERVER-ANNOTATION>
									<OPERATION-REF DEST="CLIENT-SERVER-OPERATION">/Interfaces/NVBLOCK_SWC_NVMM_CSIF/SAVE_Speed_Data</OPERATION-REF>
								</CLIENT-SERVER-ANNOTATION>
							</CLIENT-SERVER-ANNOTATIONS>
						</R-PORT-PROTOTYPE>
					</PORTS>

				</NV-BLOCK-SW-COMPONENT-TYPE>


				<!-- <SERVICE-SW-COMPONENT-TYPE>
					<SHORT-NAME>APP_DCM_SERVICE_SWC</SHORT-NAME>
				</SERVICE-SW-COMPONENT-TYPE> -->

			</ELEMENTS>
		</AR-PACKAGE>
		<AR-PACKAGE>
			<SHORT-NAME>Interfaces</SHORT-NAME>
			<ELEMENTS>
				<!-- lidar sensor actuator to ECU Abstraction SWC -->
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>LIDAR_SA_SRIF</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>LIDAR_Data</SHORT-NAME>
							<SW-DATA-DEF-PROPS>
								<SW-DATA-DEF-PROPS-VARIANTS>
									<SW-DATA-DEF-PROPS-CONDITIONAL>
										<SW-IMPL-POLICY>QUEUED</SW-IMPL-POLICY>
									</SW-DATA-DEF-PROPS-CONDITIONAL>
								</SW-DATA-DEF-PROPS-VARIANTS>
							</SW-DATA-DEF-PROPS>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
							<INIT-VALUE>
								<NUMERICAL-VALUE-SPECIFICATION>
									<VALUE>10</VALUE>
								</NUMERICAL-VALUE-SPECIFICATION>
							</INIT-VALUE>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<!-- SPEED sensor actuator to ECU Abstraction SWC -->
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SPEED_SA_SRIF</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>SPEED_Data</SHORT-NAME>
							<SW-DATA-DEF-PROPS>
								<SW-DATA-DEF-PROPS-VARIANTS>
									<SW-DATA-DEF-PROPS-CONDITIONAL>
										<SW-IMPL-POLICY>QUEUED</SW-IMPL-POLICY>
									</SW-DATA-DEF-PROPS-CONDITIONAL>
								</SW-DATA-DEF-PROPS-VARIANTS>
							</SW-DATA-DEF-PROPS>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
							<INIT-VALUE>
								<NUMERICAL-VALUE-SPECIFICATION>
									<VALUE>10</VALUE>
								</NUMERICAL-VALUE-SPECIFICATION>
							</INIT-VALUE>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>

				<!-- Warning LED State from SA to ECU Abstraction SWC -->
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>Warning_LED_State_SA_ECU_ABS_SRIF</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Warning_LED_state</SHORT-NAME>
							<SW-DATA-DEF-PROPS>
								<SW-DATA-DEF-PROPS-VARIANTS>
									<SW-DATA-DEF-PROPS-CONDITIONAL>
										<SW-IMPL-POLICY>STANDARD</SW-IMPL-POLICY>
									</SW-DATA-DEF-PROPS-CONDITIONAL>
								</SW-DATA-DEF-PROPS-VARIANTS>
							</SW-DATA-DEF-PROPS>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint8</TYPE-TREF>
							<INIT-VALUE>
								<NUMERICAL-VALUE-SPECIFICATION>
									<VALUE>0</VALUE>
								</NUMERICAL-VALUE-SPECIFICATION>
							</INIT-VALUE>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>

				<PARAMETER-INTERFACE>
					<SHORT-NAME>CLIBRATION_DISTANCE_PARAMETERIF</SHORT-NAME>
					<PARAMETERS>
						<PARAMETER-DATA-PROTOTYPE>
							<SHORT-NAME>D1</SHORT-NAME>
							<SW-DATA-DEF-PROPS>
								<SW-DATA-DEF-PROPS-VARIANTS>
									<SW-DATA-DEF-PROPS-CONDITIONAL>
										<SW-IMPL-POLICY>CONST</SW-IMPL-POLICY>
									</SW-DATA-DEF-PROPS-CONDITIONAL>
								</SW-DATA-DEF-PROPS-VARIANTS>
							</SW-DATA-DEF-PROPS>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</PARAMETER-DATA-PROTOTYPE>
						<PARAMETER-DATA-PROTOTYPE>
							<SHORT-NAME>D2</SHORT-NAME>
							<SW-DATA-DEF-PROPS>
								<SW-DATA-DEF-PROPS-VARIANTS>
									<SW-DATA-DEF-PROPS-CONDITIONAL>
										<SW-IMPL-POLICY>CONST</SW-IMPL-POLICY>
									</SW-DATA-DEF-PROPS-CONDITIONAL>
								</SW-DATA-DEF-PROPS-VARIANTS>
							</SW-DATA-DEF-PROPS>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</PARAMETER-DATA-PROTOTYPE>
						<PARAMETER-DATA-PROTOTYPE>
							<SHORT-NAME>D3</SHORT-NAME>
							<SW-DATA-DEF-PROPS>
								<SW-DATA-DEF-PROPS-VARIANTS>
									<SW-DATA-DEF-PROPS-CONDITIONAL>
										<SW-IMPL-POLICY>CONST</SW-IMPL-POLICY>
									</SW-DATA-DEF-PROPS-CONDITIONAL>
								</SW-DATA-DEF-PROPS-VARIANTS>
							</SW-DATA-DEF-PROPS>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</PARAMETER-DATA-PROTOTYPE>
					</PARAMETERS>
				</PARAMETER-INTERFACE>
				<NV-DATA-INTERFACE>
					<SHORT-NAME>NV_DATA_IF</SHORT-NAME>
					<NV-DATAS>
						<!-- save Action/sensor values 3 NV_Data -->
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Action</SHORT-NAME>
							<SW-DATA-DEF-PROPS>
								<SW-DATA-DEF-PROPS-VARIANTS>
									<SW-DATA-DEF-PROPS-CONDITIONAL>
										<SW-IMPL-POLICY>QUEUED</SW-IMPL-POLICY>
									</SW-DATA-DEF-PROPS-CONDITIONAL>
								</SW-DATA-DEF-PROPS-VARIANTS>
							</SW-DATA-DEF-PROPS>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint8</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>

						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>LIDAR_Data</SHORT-NAME>
							<SW-DATA-DEF-PROPS>
								<SW-DATA-DEF-PROPS-VARIANTS>
									<SW-DATA-DEF-PROPS-CONDITIONAL>
										<SW-IMPL-POLICY>QUEUED</SW-IMPL-POLICY>
									</SW-DATA-DEF-PROPS-CONDITIONAL>
								</SW-DATA-DEF-PROPS-VARIANTS>
							</SW-DATA-DEF-PROPS>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Speed_Data</SHORT-NAME>
							<SW-DATA-DEF-PROPS>
								<SW-DATA-DEF-PROPS-VARIANTS>
									<SW-DATA-DEF-PROPS-CONDITIONAL>
										<SW-IMPL-POLICY>QUEUED</SW-IMPL-POLICY>
									</SW-DATA-DEF-PROPS-CONDITIONAL>
								</SW-DATA-DEF-PROPS-VARIANTS>
							</SW-DATA-DEF-PROPS>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>

					</NV-DATAS>
				</NV-DATA-INTERFACE>

				<CLIENT-SERVER-INTERFACE>
					<SHORT-NAME>NVBLOCK_SWC_NVMM_CSIF</SHORT-NAME>
					<IS-SERVICE>true</IS-SERVICE>
					<OPERATIONS>
						<CLIENT-SERVER-OPERATION>
							<SHORT-NAME>SAVE_Action</SHORT-NAME>
							<ARGUMENTS>
								<ARGUMENT-DATA-PROTOTYPE>
									<SHORT-NAME>Action</SHORT-NAME>
									<SW-DATA-DEF-PROPS>
										<SW-DATA-DEF-PROPS-VARIANTS>
											<SW-DATA-DEF-PROPS-CONDITIONAL>
												<SW-IMPL-POLICY>QUEUED</SW-IMPL-POLICY>
											</SW-DATA-DEF-PROPS-CONDITIONAL>
										</SW-DATA-DEF-PROPS-VARIANTS>
									</SW-DATA-DEF-PROPS>
									<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint8</TYPE-TREF>
									<DIRECTION>IN</DIRECTION>
								</ARGUMENT-DATA-PROTOTYPE>
							</ARGUMENTS>
						</CLIENT-SERVER-OPERATION>
						<CLIENT-SERVER-OPERATION>
							<SHORT-NAME>SAVE_LIDAR_Data</SHORT-NAME>
							<ARGUMENTS>
								<ARGUMENT-DATA-PROTOTYPE>
									<SHORT-NAME>LIDAR_Data</SHORT-NAME>
									<SW-DATA-DEF-PROPS>
										<SW-DATA-DEF-PROPS-VARIANTS>
											<SW-DATA-DEF-PROPS-CONDITIONAL>
												<SW-IMPL-POLICY>QUEUED</SW-IMPL-POLICY>
											</SW-DATA-DEF-PROPS-CONDITIONAL>
										</SW-DATA-DEF-PROPS-VARIANTS>
									</SW-DATA-DEF-PROPS>
									<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
									<DIRECTION>IN</DIRECTION>
								</ARGUMENT-DATA-PROTOTYPE>
							</ARGUMENTS>
						</CLIENT-SERVER-OPERATION>
						<CLIENT-SERVER-OPERATION>
							<SHORT-NAME>SAVE_Speed_Data</SHORT-NAME>
							<ARGUMENTS>
								<ARGUMENT-DATA-PROTOTYPE>
									<SHORT-NAME>Speed_Data</SHORT-NAME>
									<SW-DATA-DEF-PROPS>
										<SW-DATA-DEF-PROPS-VARIANTS>
											<SW-DATA-DEF-PROPS-CONDITIONAL>
												<SW-IMPL-POLICY>QUEUED</SW-IMPL-POLICY>
											</SW-DATA-DEF-PROPS-CONDITIONAL>
										</SW-DATA-DEF-PROPS-VARIANTS>
									</SW-DATA-DEF-PROPS>
									<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
									<DIRECTION>IN</DIRECTION>
								</ARGUMENT-DATA-PROTOTYPE>
							</ARGUMENTS>
						</CLIENT-SERVER-OPERATION>
					</OPERATIONS>
				</CLIENT-SERVER-INTERFACE>

				<CLIENT-SERVER-INTERFACE>
					<SHORT-NAME>APP_DCM_LOG_CSIF</SHORT-NAME>
					<IS-SERVICE>true</IS-SERVICE>
					<OPERATIONS>
						<CLIENT-SERVER-OPERATION>
							<SHORT-NAME>LOG_Action</SHORT-NAME>
							<ARGUMENTS>
								<ARGUMENT-DATA-PROTOTYPE>
									<SHORT-NAME>Action</SHORT-NAME>
									<SW-DATA-DEF-PROPS>
										<SW-DATA-DEF-PROPS-VARIANTS>
											<SW-DATA-DEF-PROPS-CONDITIONAL>
												<SW-IMPL-POLICY>QUEUED</SW-IMPL-POLICY>
											</SW-DATA-DEF-PROPS-CONDITIONAL>
										</SW-DATA-DEF-PROPS-VARIANTS>
